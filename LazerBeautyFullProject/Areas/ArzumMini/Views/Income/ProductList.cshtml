                                                                    <th>Maya qiyməti</th>
                                                                    <th>Satış qiyməti</th>
                                                                </tr>
                                                            </tfoot>
                                                            <tbody>

                                                                @foreach (var item in Model.ArzumBeautyStock)
                                                                {
                                                             
                                                                        <tr>
                                                                            <th>@item.AddingDate.ToString("dd.MM.yyyy")</th>
                                                                            <td>@item.AppUser.UserName</td>
