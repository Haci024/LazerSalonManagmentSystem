@model DTO.DTOS.StockDTO.FilialDepoDTO
@{
    ViewData["Title"] = "ProductList";
}


<style>
    .size {
        width: 30px;
        height: 10px;
    }
</style>
<div class="container-fluid">
    <div class="row clearfix">
        <div class="col-xs-12 col-sm-12">
            <div class="card">
                <div class="body">
                    <div>
                        <ul class="nav nav-tabs" role="tablist">
                            <li role="presentation" class="active"><a href="#home" aria-controls="home" role="tab" data-toggle="tab">Arzum Mini</a></li>
                            <li role="presentation"><a href="#profile_settings" aria-controls="settings" role="tab" data-toggle="tab">Arzum Beauty</a></li>
                            <li role="presentation"><a href="#change_password_settings" aria-controls="pasword_settings" role="tab" data-toggle="tab">Arzum Estetik</a></li>
                        </ul>

                        <div class="tab-content">

                            <div role="tabpanel" class="tab-pane fade in active" id="home">
                                <div class="panel panel-default panel-post">
                                    <div class="row clearfix">
                                        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                         
                                                <div class="header">
                                                    <div style="display:flex;justify-content:space-between;align-items:center">
                                                        <h2>
                                                            Arzum Mini
                                                        </h2>
                                                        <div style="padding-right:30px;">
                                                            <a asp-action="ArchiveProducts" class="btn btn-outline-warning waves-effect ">
                                                                <i class="material-icons">list</i>

                                                            </a>
                                                            <a asp-action="AddProduct" class="btn bg-deep-purple waves-effect ">
                                                                <i class="material-icons">add</i>

                                                            </a>
                                                        </div>
                                                    </div>
                                                    <ul class="header-dropdown m-r--5">
                                                        <li class="dropdown">
                                                            <a href="javascript:void(0);" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">
                                                                <i class="material-icons">more_vert</i>
                                                            </a>

                                                        </li>
                                                    </ul>
                                                </div>
                                                <div class="body">
                                                    <div class="table-responsive">
                                                        <table class="table table-bordered table-striped table-hover js-basic-example dataTable">
                                                            <thead>
                                                                <tr>
                                                                    <th>Qeydiyyat tarixi</th>
                                                                    <th>Qeydiyyatçı</th>
                                                                    <th>Məhsul</th>
                                                                    <th>Satılma sayı</th>
                                                                    <th>Mövcud say</th>
                                                                    <th>Ümumi gətirilib</th>
                                                                    <th>Maya qiyməti</th>
                                                                    <th>Satış qiyməti</th>
                                                                <th>Əməliyyatlar</th>
                                                                </tr>
                                                            </thead>
                                                            <tfoot>
                                                                <tr>
                                                                    <th>Qeydiyyat tarixi</th>
                                                                    <th>Qeydiyyatçı</th>
                                                                    <th>Məhsul</th>
                                                                    <th>Satılma sayı</th>
                                                                    <th>Mövcud say</th>
                                                                    <th>Ümumi gətirilib</th>
                                                                    <th>Maya qiyməti</th>
                                                                    <th>Satış qiyməti</th>
                                                                <th>Əməliyyatlar</th>
                                                                </tr>
                                                            </tfoot>
                                                            <tbody>

                                                                @foreach (var item in Model.ArzumMiniStock)
                                                                {
                                                              
                                                                        <tr>
                                                                            <th>@item.AddingDate.ToString("dd.MM.yyyy")</th>
                                                                            <td>@item.AppUser.UserName</td>
                                                                            <td>@item.ProductName</td>
                                                                            <td>@item.SellingCount</td>
                                                                            <td>@item.RemainCount</td>
                                                                            <td>@item.TotalCount</td>
                                                                            <td>@item.BuyingPrice.ToString("#0.##") AZN</td>
                                                                            <td>@item.SellingPrice.ToString("#0.##") AZN</td>
                                                                    <td>
                                                                        <a asp-controller="Income" asp-action="UpdateProductCount" asp-route-ProductId="@item.Id" class="btn btn-success">
                                                                            <i class="material-icons">add</i>
                                                                        </a>
                                                                    </td>
                                                                        </tr>
                                                                    

                                                                }





                                                            </tbody>
                                                        </table>
                                                    </div>
                                                </div>
                                            </div>


                                      
                                    </div>
                                </div>

                            </div>
                            <div role="tabpanel" class="tab-pane fade in " id="profile_settings">
                                <div class="panel panel-default panel-post">
                                    <div class="row clearfix">
                                        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                          
