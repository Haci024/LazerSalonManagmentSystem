</div>
<div id="resultForLazer"></div>
 
<script src="~/js/pages/tables/jquery-datatable.js"></script>

<script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
<script>
    $(document).ready(function () {
        $("#calculateButton").on("click", function () {
            var startDate = $("#startDate").val();
            var endDate = $("#endDate").val();
            
            $.ajax({
                url: "/ArzumEstetic/Kassa/CalculateTotalPrice",
                type: "POST",
                data: { startDate: startDate, endDate: endDate},
                success: function (response) {
                    $("#LazerEpilyasiya").val(response.lazerEarning + " " + "AZN");
                    $("#Pirsinq").val(response.pirsinqEarning + " " + "AZN");
                    $("#Lipucka").val(response.lipuckaEarning + " " + "AZN");
                    $("#Solarium").val(response.solariumEarning + " " + "AZN");
                    $("#OutMoney").val(response.outMoney + " " + "AZN");
                    $("#BodyShaping").val(response.bodyShapingarning + " " + "AZN");
                    $("#Cosmetology").val(response.cosmetology + " " + "AZN");
                    $("#Totalİmpuls").val(response.impulsCount);
                    $("#İncome").val(response.incomeMoney + " " + "AZN");
                    $("#totalEarning").val(response.totalEarning + " " + "AZN");
                   
                },
                error: function () {
                    Swal.fire(
                        'Xəbərdarlıq!',
                        'Tarixləri düzgün qeyd edin!',
                        'warning'
                    )
                }
            });
        });
     

    });
</script>
